<h1>Listing admin_property_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_property_types.each do |admin_property_type| %>
      <tr>
        <td><%= admin_property_type.name %></td>
        <td><%= link_to 'Show', admin_property_type %></td>
        <td><%= link_to 'Edit', edit_admin_property_type_path(admin_property_type) %></td>
        <td><%= link_to 'Destroy', admin_property_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property type', new_admin_property_type_path %>
