<h1>Listing admin_features</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_features.each do |admin_feature| %>
      <tr>
        <td><%= admin_feature.name %></td>
        <td><%= link_to 'Show', admin_feature %></td>
        <td><%= link_to 'Edit', edit_admin_feature_path(admin_feature) %></td>
        <td><%= link_to 'Destroy', admin_feature, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feature', new_admin_feature_path %>
